<li class="directory-item document {{@root.documentCls}} flexrow" data-entry-id="{{this.id}}" data-document-id="{{this.id}}">
  {{#if this.thumbnail}}
  <img class="thumbnail" title="{{this.directoryName}}" data-src="{{this.thumbnail}}"/>
  {{/if}}
  <h4 class="entry-name document-name"><a>{{this.directoryName}}</a></h4>
</li>

